{
  "name": "leddit",
  "version": "1.0.0",
  "description": "a reddit clone created using react/next, typescript, nodeJS/express, typeorm, and PostgreSQL",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon dist/index.js",
    "watch": "tsc -w",
    "build": "rm -rf dist && tsc",
    "dev:watch": "concurrently \"npm run watch\" \"npm run dev\"",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "migration:generate": "npm run build && npm run typeorm migration:generate -- -n"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jomark031497/leddit.git"
  },
  "author": "Jomark Pangan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jomark031497/leddit/issues"
  },
  "homepage": "https://github.com/Jomark031497/leddit#readme",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-redis": "^0.0.18",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/ioredis": "^4.28.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.12",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "babel-eslint": "^10.1.0",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.27.1",
    "connect-redis": "^6.1.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "ioredis": "^4.28.5",
    "multer": "^1.4.4",
    "multer-storage-cloudinary": "^4.0.0",
    "passport": "^0.5.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "redis": "^4.0.4",
    "typeorm": "^0.2.41",
    "typeorm-seeding": "^1.6.1"
  },
  "engines": {
    "node": "16.13.1",
    "npm": "8.1.2"
  }
}
